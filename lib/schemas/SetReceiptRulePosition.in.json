                },
                "RuleName": {
                    "description": "The name of the receipt rule to reposition.",
                    "type": "string"
                },
                "RuleSetName": {
                    "description": "The name of the receipt rule set that contains the receipt rule to reposition.",
                    "type": "string"
                }
